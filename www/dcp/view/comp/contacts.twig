<section
	class="col-lg-12 connectedSortable">
	<!-- Custom tabs (Charts with tabs)-->
	<div class="card">
		<div class="card-header">
			<h3 class="card-title">
				<i class="fas fa-chart-pie mr-1"></i>
				Enquiries
			</h3>
			<div class="card-tools"></div>
		</div>
		<div class="card-body">
			<table id="datatable" class="table table-bordered table-hover">
				<thead>
					<tr>
						<th>Sl. No.</th>
						<th>EnqNo.</th>
						<th>Name</th>
						<th>E-Mail</th>
						<th>Phone</th>
						<th>Message</th>
						<th>Request Date/Time</th>
					</tr>
				</thead>
				<tbody>
					{% for query in data %}
						<tr>
							<td>{{loop.index}}</td>
							<td>{{query.id}}</td>
							<td>{{query.name}}</td>
							<td>{{query.email}}</td>
							<td>{{query.phone}}</td>
							<td>{{query.bizdetails}}</td>
							<td>{{query.added_on}}</td>
						</tr>
					{% endfor %}
				</tbody>
			</table>
		</div>
	</div>
</section>
