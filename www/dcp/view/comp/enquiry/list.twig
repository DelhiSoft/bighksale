<section
	class="col-lg-12 connectedSortable">
	<!-- Custom tabs (Charts with tabs)-->
	<div class="card">
		<div class="card-header">
			<h3 class="card-title">
				<i class="fas fa-chart-pie mr-1"></i>
			Enquiries
			</h3>
			
		</div>

		<div class="card-body">
			<table class="table">
			<tr>
				<th>Sl. No.</th>
                                <th>
                                   Name
                                </th>
                                <th>
                                   E-Mail
                                </th>
                                <th>
                                   Phone
                                </th>
                                <th>
                                   Date
                                </th>
			</tr>
				{% for info in data %}
					<tr>
                                            <td>
                                                {{ loop.index }}
                                            </td>
						<td>
                                                    {{ info.name }}
                                                </td>
						<td>
                                                    {{ info.email }}
                                                </td>
						<td>
                                                {{ info.phone }}
                                                </td>
                                                <td>
                                                    {{ info.updated_on|date('d-M-Y') }}
                                                </td>
					</tr>
				{% endfor %}
			</table>
		</div>
	</div>
</section>
