<section
	class="col-lg-12 connectedSortable">
	<!-- Custom tabs (Charts with tabs)-->
	<div class="card">
		<div class="card-header">
			<h3 class="card-title">
				<i class="fas fa-chart-pie mr-1"></i>
				Orders [{{data[0]['invno']}}]
			</h3>
		</div>

		<div class="card-body">
			<table class="table">
			<tr>
				<th>Sl. No.</th>
				<th>Name</th>
				<th>Contact Number</th>
				<th>E-Mail</th>
                                <th>Date</th>
			</tr>
				{% for info in infos %}
                	<tr>
						<td>{{loop.index}}</td>
						<td>{{info.name}}</td>
						<td>{{info.phone}}</td>
						<td>{{info.email}}</td>
                                                <td>{{ info.date }}</td>
					</tr>
				{% endfor %}
			</table>
		</div>
	</div>
</section>
